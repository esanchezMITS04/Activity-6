<!DOCTYPE html>
<html>
  <head>
    <title>Workout</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
        <br/>
        <%= form_tag "/client_workouts/find" do %>
        <%= text_field_tag :search_string %>
        <%=submit_tag :search%>
        <%end%>
        <br/>
    <%= yield %>
  </body>
</html>
